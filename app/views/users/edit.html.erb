<h1>New book</h1>
 <%= form_with model: @book do |f| %>
   <h4>Title</h4>
    <%= f.text_field :title %>
   <h4>Opinion</h4>
    <%= f.text_area :body %>
   <%= f.submit 'create book' %>
 <% end %>

<h1>User info</h1>
<%= form_with model:@user do |f| %>
 <p>Name</p>
  <%= f.text_field :name %>
 <p>Image</p>
  <%= f.file_field :profile_imae %>
 <p>Introduction</p>
  <%= f.text_field :introduction %>
 <%= f.submit "create book" %>
<% end %>

